<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_launch">

    <fragment
        android:id="@+id/nav_launch"
        android:name="com.missclick.gameclick.ui.launch.LaunchFragment"
        android:label="@string/menu_launch"
        tools:layout="@layout/fragment_launch" />

    <fragment
        android:id="@+id/nav_main"
        android:name="com.missclick.gameclick.ui.main.MainFragment"
        android:label="@string/menu_main"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/nav_win"
        android:name="com.missclick.gameclick.ui.win.WinFragment"
        android:label="@string/menu_win"
        tools:layout="@layout/fragment_win" />

    <fragment
        android:id="@+id/nav_lost"
        android:name="com.missclick.gameclick.ui.lost.LostFragment"
        android:label="@string/menu_lose"
        tools:layout="@layout/lost_fragment" />


</navigation>